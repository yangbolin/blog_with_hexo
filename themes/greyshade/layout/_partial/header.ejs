<div class="profilepic">
  <script src="<%- config.root %>js/md5.js"></script>
  <script type="text/javascript">
    $(function(){
	$('.profilepic').append(
          "<img src='http://www.gravatar.com/avatar/"
          + MD5("<%- config.email %>")
          + "?s=120' alt='Profile Picture' style='width: 120px;' />");
    });
  </script>
</div><!-- profilepic -->
<h2><a href="<%- config.root %>"><%= config.title %></a></h2>
<p class="subtitle"><%= config.subtitle %></p>
<nav id="main-nav">
  <ul class="menu">
  <% for (var i in theme.menu){ %>
      <li><a href="<%- theme.menu[i] %>"><b><%= i %></b></a></li>
  <% } %>
  </ul>
  <% if(config.description) { %>
  <section class="aboutme">
    <p>
      <%= config.description %>
    </p>
  </section>
  <% } %>
</nav><!-- main-nav -->
<% if(theme.social.enable) { %>
<nav id="sub-nav">
  <div class="social">
    <% if(theme.social.tsina) { %>
      <a class="tsina" href="http://weibo.com/<%- theme.social.tsina %>"
         title="Weibo">Weibo</a>
    <% } %>
    <% if(theme.social.facebook) { %>
      <a class="facebook" href="http://www.facebook.com/<%- theme.social.facebook %>"
         title="Facebook">Facebook</a>
    <% } %>
    <% if(theme.social.google) { %>
      <a class="google" href="http://plus.google.com/<%- theme.social.google %>"
         title="Google+">Google+</a>
    <% } %>
    <% if(theme.social.twitter) { %>
      <a class="twitter" href="http://twitter.com/<%- theme.social.twitter %>"
         title="Twitter">Twitter</a>
    <% } %>
    <% if(theme.social.github) { %>
      <a class="github" href="http://github.com/<%- theme.social.github %>"
         title="Github">Github</a>
    <% } %>
    <% if(theme.social.email) { %>
        <a class="email" href="mailto:<%- config.email%>" title="Email">Email</a>
    <% } %>
    <% if(theme.social.rss) { %>
        <a class="rss" href="<%- config.root%>atom.xml" title="RSS">RSS</a>
    <% } %>
  </div>
</nav><!-- sub-nav -->
<% } %>
